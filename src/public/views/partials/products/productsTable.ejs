<table class="productsTable table table-bordered bg-color mx-auto">
	<tr class="text-light">
		<th>Image</th>
		<th>Title</th>
		<th>Price</th>
	</tr>
	<% if (products && products.length > 0) { %> <% products.forEach(product => { %>
	<tr>
		<td><img class="productImg" src="<%= product.imageUrl %>" alt="Img" /></td>
		<td><%= product.title %></td>
		<td><%= product.price %></td>
	</tr>
	<%})%> <% }else{%>
	<tr>
		<td colspan="4">
			<h3>No products found.</h3>
		</td>
	</tr>
	<% }%>
</table>